    // Represents a single value in a chain stored in one hash bucket.
    private class HashEntry<AnyKey extends Comparator<AnyKey>, AnyValue> {
        public AnyKey data;
        public AnyValue value;
        public HashEntry<AnyKey, AnyValue> next;

        public HashEntry(AnyKey data) {
            this(data,null, null);
        }

        public HashEntry(AnyKey data, AnyValue value, HashEntry<AnyKey, AnyValue> next) {
            this.data = data;
            this.next = next;
            this.value = value;
        }
    }
	@Override
	public Iterator<AnyKey> iterator() {
		// TODO Auto-generated method stub
		return new MyIterator();
	}
	
	 private class MyIterator implements Iterator<AnyKey>
	   {
	      Stack<HashEntry<AnyKey, AnyValue>> stk = new Stack<HashEntry<AnyKey, AnyValue>>();

	      public MyIterator()
	      {
	         for(int i = 0; i  < elementData.length; i++){
	        	 if(elementData[i] != null){
	        		 stk.push(elementData[i]);
	        		 break;
	        	 }
	         }
	      }
	      public boolean hasNext()
	      {
	         return !stk.isEmpty();
	      }

	      public AnyKey next()
	      {
	    	HashEntry<AnyKey, AnyValue> cur = stk.peek();
	    	
	    	for(int i = 0; i < elementData.length; i++){
	    		if(elementData[i] != null && elementData[i] != cur){
	    			stk.push(elementData[i]);
	    			cur = elementData[i];
	    			break;
	    		}if(elementData[i] == cur && elementData[i].next != null){
	    			stk.push(elementData[i].next);
	    		}
	    	}
	    	
	        return cur.data;
	        
	      }//end of next()
	   }

}
